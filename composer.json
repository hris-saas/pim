{
    "name": "hris-saas/pim",
    "description": "HRis SaaS PIM Package.",
    "keywords": [
        "hris-saas",
        "package",
        "pim"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Bertrand Kintanar",
            "email": "bertrand.kintanar@gmail.com",
            "role": "Creator & Designer"
        }
    ],
    "support": {
        "issues": "https://gitlab.com/hris-saas/bugs/issues",
        "source": "https://gitlab.com/hris-saas/pim"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../auth"
        },
        {
            "type": "vcs",
            "url": "../baum"
        },
        {
            "type": "path",
            "url": "../core"
        }
    ],
    "require": {
        "hris-saas/auth": "*",
        "hris-saas/baum": "*",
        "hris-saas/core": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "laravel/laravel": "^7.0",
        "phpunit/phpunit": "^8.0",
        "jaschilz/php-coverage-badger": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "HRis\\PIM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HRis\\PIM\\Tests\\UnitTests\\": "tests/unit-tests/",
            "HRis\\PIM\\Tests\\Eloquents\\": "tests/eloquents/"
        },
        "classmap": [
            "tests/Test.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "HRis\\PIM\\Providers\\PIMServiceProvider",
                "HRis\\PIM\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
